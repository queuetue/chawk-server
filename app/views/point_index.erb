<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Series '<%=  @addr.address %>' / Point '<%=  @point.id %>'</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="row">
<div class="col-lg-6">
  <h2>Point Data</h2>
  <div class="table-responsive">
    <table class="table table-bordered table-hover tablesorter">
      <thead>
        <tr>
          <th>Key <i class="fa fa-sort"></i></th>
          <th>Value <i class="fa fa-sort"></i></th>
        </tr>
      </thead>
      <tbody>
        <tr><td><strong>ID</strong></td><td><%=  @point.id %></td></tr>
        <tr><td><strong>Value</strong></td><td><%=  @point.value %></td></tr>
        <tr><td><strong>Observed</strong></td><td><%=  Time.at(@point.observed_at).strftime("%b %-d, %Y %H:%M:%S.%L ") %></td></tr>
        <tr><td><strong>Recorded</strong></td><td><%=  @point.recorded_at.strftime("%b %-d, %Y ") %></td></tr>
        <tr>
      </tbody>
    </table>
  </div>
</div>


<div class="row">
  <div class="col-lg-12">
  <table>
  </div>
</div>

<!-- SB Admin Scripts - Include with every page -->
<script src="/js/sb-admin.js"></script>
